{
  "name": "nestjs-fast-template",
  "version": "0.0.1",
  "description": "NestJS template for fast start (Testingless) with fast tecnologies (Fastify, SWC, BiomeJS)",
  "author": "Ivan Ulloque (@ZUR1C4T0)",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "biome format --write ./src",
    "start": "nest start",
    "dev": "nest start --watch",
    "debug": "nest start --debug --watch",
    "prod": "node dist/main",
    "lint": "biome lint --apply ./src"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.2",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.3.2",
    "@nestjs/platform-fastify": "^10.3.2",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@swc/cli": "^0.3.9",
    "@swc/core": "^1.4.0",
    "@types/node": "^20.11.16",
    "source-map-support": "^0.5.21",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}