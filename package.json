    "@biomejs/biome": "^1.8.3",
    "@nestjs/cli": "^10.4.2",
    "@nestjs/schematics": "^10.1.2",
    "@swc/cli": "^0.3.14",
    "@swc/core": "^1.7.0",
    "@types/node": "^20.14.11",
    "source-map-support": "^0.5.21",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.3"
